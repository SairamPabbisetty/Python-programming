   "source": [
    "# amstrong\n",
    "\n",
    "n = int(input(\"Enter a number : \"))\n",
    "l = len(str(n))\n",
    "temp = n\n",
    "sum = 0\n",
    "while(temp>0) :\n",
    "    rem = temp%10\n",
    "    sum += rem**l\n",
    "    temp = temp//10\n",
    "if(sum==n) :\n",
    "    print(\"amstrong\")\n",
    "else :\n",
    "    print(\"not amstrong\")"
   ]
