   "source": [
    " # Find sum and avg of elements in a tuple\n",
    "    \n",
    "num = eval(input(\"Enter elements in the tuple() : \"))\n",
    "\n",
    "sum=0\n",
    "\n",
    "for i in range(len(num)) :\n",
    "    sum+=num[i]\n",
    "    \n",
    "print(\"Sum of numbers is \",sum)\n",
    "print(\"Average of numbers is \",sum/len(num))"
   ]
